{
	"name": "femundfilou/kirby-asset-manager",
	"description": "Add CSS and JS per (block) snippet",
	"license": "MIT",
	"homepage": "https://github.com/femundfilou/kirby-asset-manager",
	"type": "kirby-plugin",
	"version": "1.0.0",
	"authors": [
		{
			"name": "Justus Kraft",
			"email": "justus@femundfilou.com"
		}
	],
	"require": {
		"getkirby/composer-installer": "^1.1"
	},
	"scripts": {
		"fix": "php-cs-fixer fix --config .php-cs-fixer.dist.php"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^3.37"
	},
	"autoload": {
		"psr-4": {
			"Femundfilou\\AssetManager\\": "lib/"
		}
	},
	"config": {
		"optimize-autoloader": true,
		"allow-plugins": {
			"getkirby/composer-installer": true
		}
	},
	"extra": {
		"installer-name": "asset-manager"
	}
}
